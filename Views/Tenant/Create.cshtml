@model RentAutomation.Models.Tenant

@{
    ViewData["Title"] = "Create Tenant";
}

<h1>Create Tenant</h1>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="TenantName" class="control-label"></label>
        <input asp-for="TenantName" class="form-control" />
        <span asp-validation-for="TenantName" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="TenantPhone" class="control-label"></label>
        <input asp-for="TenantPhone" class="form-control" />
        <span asp-validation-for="TenantPhone" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="TenantHouseNo" class="control-label"></label>
        <input asp-for="TenantHouseNo" class="form-control" />
        <span asp-validation-for="TenantHouseNo" class="text-danger"></span>
    </div>

  

    <div class="form-group">
        <label asp-for="Rent" class="control-label"></label>
        <input asp-for="Rent" class="form-control" />
        <span asp-validation-for="Rent" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Deposit" class="control-label"></label>
        <input asp-for="Deposit" class="form-control" />
        <span asp-validation-for="Deposit" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="EbPerUnit" class="control-label"></label>
        <input asp-for="EbPerUnit" class="form-control" />
        <span asp-validation-for="EbPerUnit" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Water" class="control-label"></label>
        <input asp-for="Water" class="form-control" />
        <span asp-validation-for="Water" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Create</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
